class Foo {
    int x 
    int y 

    init () {
        x = 0
        y = 0
    }

    func setX (int val) {
        x = val
    }

    func print () {
        putchar(48+x)
        putchar(48+y)
    }

    func setY (int val) {
        y = val
    }

    int getY () {
        return y
    }
}

Foo foo
foo.setX(3)
foo.print()
foo.setY(6)
putchar(foo.getY() + 48)