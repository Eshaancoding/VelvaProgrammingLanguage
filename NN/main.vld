class Array {
public:    
    ptr<float> x
    int len
    
    init (int length) {
        len = length
        x = malloc(length)
    }

    func set (int index, float value) {
        x[index] = value
    }

    func print () {
        for (int i = 0; i < len; i++) {
            putchar((100*x[i]/100/100/100/100/100/100/100/100)+48) 
        }
    } 
}

Array arr (5)
arr.set(0, 3)
arr.print()
